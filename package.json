{
  "name": "movies",
  "version": "0.36.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint ./src",
    "tsc": "tsc",
    "lint:fix": "eslint . --fix",
    "build-js": "tsc",
    "firebase:dev": "ENV=dev ./bin/envscript.sh",
    "firebase:prod": "ENV=production ./bin/envscript.sh",
    "run-android-dev": "cross-env ENVFILE=.env.development react-native run-android",
    "run-android-prod": "npm run firebase:prod && cross-env ENVFILE=.env.production react-native run-android",
    "run-ios-dev": "cross-env ENVFILE=.env.development react-native run-ios",
    "run-ios-prod": "npm run firebase:prod && cross-env ENVFILE=.env.production react-native run-ios",
    "build-android-dev": "npm run build-js && cd android && cross-env ENVFILE=.env.development ./gradlew assembleRelease && cd ..",
    "build-android-prod": "npm run firebase:prod && npm run build-js && cd android && cross-env ENVFILE=.env.production ./gradlew assembleRelease && cd ..",
    "build-android-staging": "cross-env ENVFILE=.env.production npx react-native run-android --variant releaseStaging",
    "bundle-android-prod": "npm run firebase:prod && npm run build-js && cd android && cross-env ENVFILE=.env.production ./gradlew bundleRelease && cd ..",
    "postversion": "react-native-version --never-amend --skip-tag && git add . && export version=v$npm_package_version && git commit -m $version",
    "version:major": "npm --no-git-tag-version version major",
    "version:minor": "npm --no-git-tag-version version minor",
    "version:patch": "npm --no-git-tag-version version patch",
    "postinstall": "jetifier && patch-package",
    "create-tag": "git checkout master && git pull && export version=v$npm_package_version && git tag $version && git push origin --tags",
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.12.0",
    "@react-native-community/datetimepicker": "3.0.4",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/push-notification-ios": "1.8.0",
    "@react-native-firebase/app": "10.4.0",
    "@react-native-firebase/messaging": "10.4.0",
    "@react-navigation/bottom-tabs": "5.8.0",
    "@react-navigation/native": "5.7.3",
    "@react-navigation/stack": "5.9.0",
    "@types/react-native-snap-carousel": "^3.8.2",
    "axios": "0.21.0",
    "eventemitter3": "4.0.7",
    "formik": "2.1.5",
    "i18n-js": "3.7.1",
    "lodash": "4.17.20",
    "mobx": "6.0.1",
    "mobx-persist": "0.4.1",
    "mobx-react": "7.0.0",
    "native-base": "2.13.14",
    "numbro": "^2.3.2",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "react": "16.13.1",
    "react-children-addons": "1.0.9",
    "react-native": "0.63.2",
    "react-native-code-push": "^6.4.1",
    "react-native-config": "1.3.3",
    "react-native-country-picker-modal": "2.0.0",
    "react-native-datepicker": "1.7.2",
    "react-native-device-info": "^8.0.0",
    "react-native-gesture-handler": "1.7.0",
    "react-native-image-picker": "2.3.4",
    "react-native-keyboard-aware-scroll-view": "0.9.2",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^2.0.1",
    "react-native-progress": "^4.1.2",
    "react-native-progress-circle": "^2.1.0",
    "react-native-push-notification": "7.0.0",
    "react-native-reanimated": "1.13.0",
    "react-native-restart": "0.0.17",
    "react-native-safe-area-context": "3.1.7",
    "react-native-screens": "2.10.1",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "12.1.0",
    "react-native-tab-view": "^2.15.2",
    "react-native-webview": "^11.0.2",
    "react-native-youtube": "^2.0.1",
    "serializr": "2.0.3",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@babel/core": "7.11.5",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/runtime": "7.11.2",
    "@types/i18n-js": "3.0.3",
    "@types/lodash": "4.14.161",
    "@types/react-native": "0.63.12",
    "@types/react-native-datepicker": "1.7.0",
    "@types/react-native-push-notification": "5.0.5",
    "@types/react-native-restart": "0.0.0",
    "@types/yup": "0.29.6",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "babel-plugin-module-resolver": "4.0.0",
    "cross-env": "7.0.3",
    "eslint": "7.8.0",
    "eslint-config-airbnb-typescript": "9.0.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.0",
    "eslint-plugin-modules-newline": "0.0.4",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4",
    "husky": "^4.3.6",
    "metro-react-native-babel-preset": "0.63.0",
    "react-native-svg-transformer": "0.14.3",
    "react-native-version": "4.0.0",
    "typescript": "4.0.2"
  }
}
